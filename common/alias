#Set cmdprofile it self as alias
alias cmdprofile='source ~/cmdprofile/mainProfile'

#find alias
alias fa="alias | grep"

alias d="docker"
alias drm='docker rm $(docker ps -aq)'
alias drmi='docker rmi $(docker images -aq)'
alias drmin='docker rmi $(docker images -f "dangling=true" -aq)'

alias g="./gradlew"
alias a="ansible"
alias v="vagrant"


#zsh with jump plugins
alias j='jump'

#process greping with
alias psgrep='ps -ef | grep'
alias pskill='. ~/cmdprofile/common/bin/pskill.sh'


source ~/cmdprofile/common/alias

# -*- os specific profile-*-
case $OSTYPE in
    solaris10)
        ;;
    linux*)
        #echo "appling linux-specific profiles"
        source ~/cmdprofile/linux/profile
        ;;
    darwin*)
        #echo "appling osx-specific profiles"
        source ~/cmdprofile/osx/profile
        ;;
    *)
        ;;
esac
